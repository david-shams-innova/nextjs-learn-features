    "import yfinance as yf\n",
    "\n",
    "# Download data for the NIO stock\n",
    "nio = yf.Ticker(\"NIO\")\n",
    "nio_data = nio.history(period=\"max\")\n",
    "\n",
    "# The data is now stored in the nio_data DataFrame\n",
    "print(nio_data.head())"
